{
  "name": "ts-sandbox",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsc-watch --onSuccess \"node build/index.js\"",
    "build": "npx tsc",
    "start": "NODE_ENV=production node build/index.js",
    "test": "npx vitest",
    "lint": "npx eslint . --ext .ts",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "check": "npm run format && npm run lint"
  },
  "_moduleAliases": {
    "@": "build"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/ws": "^8.5.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.5",
    "typed-emitter": "^2.1.0",
    "vitest": "^0.14.2"
  },
  "dependencies": {
    "@fastify/cors": "^8.1.0",
    "@fastify/error": "^3.0.0",
    "@fastify/helmet": "^10.0.1",
    "@fastify/request-context": "^4.0.0",
    "@fastify/websocket": "^7.0.1",
    "argon2": "^0.28.7",
    "aws-sdk": "^2.1182.0",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "fastify": "^4.7.0",
    "fastify-type-provider-zod": "^1.1.7",
    "mime-types": "^2.1.35",
    "module-alias": "^2.2.2",
    "mongoose": "^6.4.4",
    "mongoose-autopopulate": "^0.16.1",
    "pino": "^8.6.1",
    "zod": "^3.19.1"
  }
}
